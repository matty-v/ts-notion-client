{
  "name": "ts-notion-client",
  "version": "1.0.8",
  "description": "A client library for interacting with the Notion API",
  "scripts": {
    "build": "tsc -p .",
    "publish": "npm run build && npm run test && npm publish",
    "test": "jest --silent",
    "e2e-test": "ts-node -e \"import('./src/__tests__/e2e-test').then(module => module.run())\"",
    "e2e-test-linux": "export $(cat .env | xargs) && npm run e2e-test",
    "e2e-test-windows": "(for /F \"tokens=*\" %i in (.env) do set %i) && npm run e2e-test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "files": [
    "dist/*"
  ],
  "main": "./dist/index.js",
  "dependencies": {
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.7",
    "@babel/preset-typescript": "^7.22.5",
    "@faker-js/faker": "^8.0.2",
    "@jest/globals": "^29.6.1",
    "@types/jest": "^29.5.2",
    "@types/jest-when": "^3.5.2",
    "@types/node": "^16.18.66",
    "jest": "^29.6.1",
    "jest-when": "^3.5.2",
    "prettier-plugin-organize-imports": "^2.3.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.2.0",
    "typescript": "^4.3.5"
  }
}
